<launch>
  <include file="$(find sr_graspatron)/launch/gazebo.launch" />
  <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py"/>
  <node name="recognizer" pkg="pocketsphinx" type="recognizer.py" output="screen">
    <param name="lm" value="$(find sr_graspatron)/dictionary/simple_commands.lm"/>
    <param name="dict" value="$(find sr_graspatron)/dictionary/simple_commands.dic"/>
  </node>
</launch>
